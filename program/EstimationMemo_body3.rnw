          , names(coeffvec))] <- 1
        lhcow <- glht(model=thisreg, linfct = matrix(hypvecm, byrow = T, nrow=1), 
          alternative="two.sided", vcov.=thisV)
        confis <- rbind(confis, 
          c("male", "level", rr, "traditional", schlevels[i], confint(lhcow)$confint[1, ]))
       #  addcova.nontrad[[2]] is "dummyInKind.Time4" 
       #  =concurrent difference relative to traditional
        diff.hypvecm <- rep(0, length(coeffvec))
        diff.hypvecm[grepl(
          paste(
