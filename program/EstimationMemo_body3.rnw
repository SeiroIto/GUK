            addcovaf.nontrad[i]
          , collapse = "|")
          , names(coeffvec))] <- 1
        lhcow <- glht(model=thisreg, linfct = matrix(diff.hypvecf, byrow = T, nrow=1), 
          alternative="two.sided", vcov.=thisV)
        confis <- rbind(confis, 
          c("female", "concurrent", rr, g, schlevels[i], confint(lhcow)$confint[1, ]))
       # nontrad level
        hypvecm <- hypvecm + diff.hypvecm
        lhcow <- glht(model=thisreg, linfct = matrix(hypvecm, byrow = T, nrow=1), 
          alternative="two.sided", vcov.=thisV)
        confis <- rbind(confis, 
          c("male", "level", rr, g, schlevels[i], confint(lhcow)$confint[1, ]))
        hypvecf <- hypvecf + diff.hypvecf
        lhcow <- glht(model=thisreg, linfct = matrix(hypvecf, byrow = T, nrow=1), 
          alternative="two.sided", vcov.=thisV)
        confis <- rbind(confis, 
